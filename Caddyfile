blog.deltaryz.com {
    tls mail@deltaryz.com
    root /srv/blog.deltaryz.com

    git {
        repo https://github.com/deltaryz/blog
        branch main
        interval 1200
        clone_args --depth 1
        pull_args --force
        then sh /srv/blog.deltaryz.com/update.sh
    }
}

